{
  "name": "pldt-vaas-frontend",
  "author": "SparkleStar International Corporation",
  "licence": "MIT",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "preinstall": "npm install --legacy-peer-deps",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx ./src",
    "lint:fix": "eslint --fix --ext .js,.jsx ./src",
    "clear-all": "rm -rf build node_modules",
    "re-start": "rm -rf build node_modules && npm install && npm start",
    "re-start-windows": "rd /s /q node_modules && npm install && npm start",
    "re-build-windows": "rd /s /q node_modules && npm install && npm run build",
    "re-build": "rm -rf node_modules && npm install && npm run build",
    "cy:open": "cypress open --env IS_TEST_ENV=true",
    "cy:run": "cypress run --env IS_TEST_ENV=true",
    "cy:open:no-env": "cypress open",
    "cy:run:no-env": " cypress run",
    "cy:open:staging": "cypress open --env IsStaging=true, IS_TEST_ENV=true",
    "cy:run:staging": "cypress run --env IsStaging=true, IS_TEST_ENV=true",
    "cy:open:staging:no-env": "cypress open --env IsStaging=true",
    "cy:run:staging:no-env": "cypress run --env IsStaging=true"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
